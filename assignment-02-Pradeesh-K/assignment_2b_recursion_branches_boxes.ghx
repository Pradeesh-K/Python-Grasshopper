                    <item name="CodeInput" type_name="gh_string" type_code="10"># this creates random branching lines with recursion
# input type: line

import Rhino.Geometry as rg

from compas.geometry import Vector, Line, Point, Box, bounding_box
from compas.geometry import Rotation

from compas_ghpython.artists import PointArtist, LineArtist
from compas_ghpython import draw_frame

import random
import math


def recursiveLine(line, iterations, axis, lines, boxes):
    
    # count down the iterations
    iterations -= 1 #'fill in here [...]'
    
    # get the start and end points of the input line
    start_point = line.start  #'fill in here [...]'
    end_point = line.end
    
    # get the vector of the input line, 2 times for the two branches
    dir1 = Vector.from_start_end(start_point, end_point)  #'fill in here [...]'
    dir2 = Vector.from_start_end(start_point, end_point)
    
    # rotate the vectors 90° in positive and negative direction around 2 axes
    R11 = Rotation.from_axis_and_angle(axis, math.pi/2)
    R12 = Rotation.from_axis_and_angle(line.vector, math.pi/2)
    R21 = Rotation.from_axis_and_angle(axis, -math.pi/2)
    R22 = Rotation.from_axis_and_angle(line.vector, math.pi/2)#I think R12 and R22 and confusing
    
    #'fill in here [...]'
    dir1.transform(R11)
    dir1.transform(R12)
    dir2.transform(R21)
    dir2.transform(R22)
    
    # for the scale, calculate a random scale between 0.7 and 0.8
    scale1 = random.uniform(0.7, 0.8)
    scale2 = random.uniform(0.7, 0.8)

    # and scale the vector according to the scale values
    dir1 = dir1 * scale1  #'fill in here [...]'
    dir2 = dir2 * scale2  #

    # finally, create two new branches
    #'fill in here [...]'
    line1_start_point =  end_point
    line1_end_point =   line1_start_point+ Point(dir1.x,dir1.y,dir1.z)  #this line was messing up the code'fill in here [...]'
    line2_start_point =  end_point
    line2_end_point =  line2_start_point + Point(dir2.x,dir2.y,dir2.z)#'fill in here [...]'
    
    line1 = Line(line1_start_point, line1_end_point)
    line2 = Line(line2_start_point, line2_end_point)

    
    # and add them to the list of branches
    lines.extend([line1,line2])  #'fill in here [...]'
   
    # create the normal vector using the cross product between the vector of the current line and dir1  #that results in a null vector so adjusting dir1,dir2 for trial
    #'fill in here [...]'
    n = line.vector.cross(dir1)
#    n2 = line.vector.cross(dir2)
    
    
    # and unitize it
    #'fill in here [...]'
    n.unitize()
#    n2.unitize()
    
    # calculate 4 corner points of the box
    pt1 = (start_point+dir1)+axis*dir1.length/2 #addition of a point and a vector results in a Vector
#    pt2 = (start_point+dir1) #'fill in here [...]'
#    pt3 = start_point #'fill in here [...]'
    pt4 = (end_point+dir2)+n*dir2.length/2 #'fill in here [...]'
    
    # create a box around the lines
    # box = rg.Box(rg.Plane.WorldYZ, [pt1, pt2, pt3, pt4])
    bbox = bounding_box([pt1, pt4])
    box = Box.from_bounding_box(bbox)
    
    # and add them to the list of boxes
    #'fill in here [...]'
    boxes.append(box)
    
    # if the iteration is higher than zero, keep calling the function recursively
    if iterations &gt; 0:
      if(random.random() &lt; 0.9): # random omission
        #'fill in here [...]'
        recursiveLine(line1, iterations, n, lines, boxes)
      if(random.random() &lt; 0.9): # random omission
        #'fill in here [...]'
        recursiveLine(line2, iterations, n, lines, boxes)



# 1. input
line = Line(Point(0.,0.,0), Point(0.,0.,1.))
# line = RhinoLine.from_geometry(line).to_compas() #  convert Rhino Line to COMPAS line
axis = Vector(1.,0.,0.)

# 2. empty lists for start
branches = []
boxes = []

# 3. call the recursive function
recursiveLine(line, iterations, axis, branches, boxes)

# 4. convert the COMPAS geometry to the Rhino Geometry
def draw_box(b):
    plane = draw_frame(b.frame)
    vertices = [rg.Point3d(*list(v)) for v in b.vertices]
    rbox =  rg.Box(plane, vertices)
    return rbox
print(boxes)
branches = [LineArtist(branch).draw() for branch in branches]
boxes = [draw_box(box) for box in boxes]
